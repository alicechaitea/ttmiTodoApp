{
    "compilerOptions": {
      "target": "ESNext",
      "module": "CommonJS",
      "lib": ["DOM", "ESNext"],
      "strict": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "outDir": "dist"
    },
    "include": ["playwright/*.ts"]
  }
  